{
  "name": "climbing-app",
  "version": "0.0.0-beta.1",
  "description": "Mythical climbing app.",
  "keywords": [
    "climbing",
    "react",
    "redux"
  ],
  "homepage": "http://github.com/notaurologist/climbing",
  "bug": "http://github.com/notaurologist/climbing/issues",
