{
  "name": "climbing-app",
  "version": "0.0.0",
  "description": "Mythical climbing site app.",
  "keywords": [
    "climbing",
    "react",
    "flow",
    "flowcheck",
    "flux",
    "postcss",
    "webpack"
  ],
  "homepage": "http://github.com/ishotfirst/climbing",
  "bug": "http://github.com/ishotfirst/climbing/issues",
  "author": "Jason LeMoine <jason.lemoine@gmail.com> (http://jasonlemoine.com)",
  "license": "MIT",
  "main": "app/js/main.js",
  "scripts": {
    "clean": "rimraf build",
    "prebuild:html": "npm run clean -s && mkdirp build -s",
    "build:html": "echo `cp app/*.html build/`",
    "open:dev": "open http://localhost:8080/webpack-dev-server/",
    "prestart": "npm run build:html -s && npm run open:dev -s",
    "start": "node server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "version:patch": "npm version patch --sign-git-tag=true -m 'Bumped version to %s'",
    "version:minor": "npm version minor --sign-git-tag=true -m 'Bumped version to %s'",
    "version:major": "npm version major --sign-git-tag=true -m 'Bumped version to %s'"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/ishotfirst/climbing.git"
  },
  "devDependencies": {
    "css-loader": "^0.9.1",
    "cssnext-loader": "^1.0.1",
    "file-loader": "^0.8.1",
    "flowcheck": "^0.2.3",
    "flowcheck-loader": "^1.0.0",
    "jsx-loader": "^0.12.2",
    "mkdirp": "^0.5.0",
    "opener": "^1.4.0",
    "postcss": "^4.0.6",
    "postcss-loader": "^0.3.0",
    "postcss-mixins": "^0.1.1",
    "postcss-nested": "^0.2.2",
    "react-hot-loader": "^1.1.7",
    "rimraf": "^2.3.2",
    "style-loader": "^0.8.3",
    "url-loader": "^0.5.5",
    "webpack": "^1.7.2",
    "webpack-dev-server": "^1.7.0"
  },
  "dependencies": {
    "cssrecipes-defaults": "^0.5.0",
    "flux": "^2.0.1",
    "normalize.css": "^3.0.2",
    "react": "^0.12.2",
    "react-router": "^0.12.4"
  },
  "private": true
}
