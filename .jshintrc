{
    // Enforcing Options
    "camelcase": true,
    "curly": true,
    "eqeqeq": true,
    "esnext": true,
    "immed": true,
    "indent": 2,
    "latedef": true,
    "newcap": true,
    "noarg": true,
    "noempty": true,
    "nonew": true,
    "quotmark": true,
    "undef": true,
    "unused": true,
    "strict": true,
    "trailing": true,
    "maxparams": 5,
    "maxdepth": 5,
    "maxstatements": 25,
    "maxcomplexity": 5,
    "maxlen": 100,
    // Environments
    "browser": true,
    "devel": true,
    "globals": {
        "require": false
    }
}